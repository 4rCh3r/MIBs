cfrJobProcessedSize OBJECT-TYPE
        SYNTAX        Gauge32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of files processed in K"
        ::= { cfrJobEntry 13 }

cfrJobQueueSize OBJECT-TYPE
        SYNTAX        Gauge32
        MAX-ACCESS    read-only
        STATUS        current
        DESCRIPTION   "Number of files remaining in the queue in K"
        ::= { cfrJobEntry 14 }
        
cfrJobGroup OBJECT-GROUP 
        OBJECTS {
                 cfrJobName,
